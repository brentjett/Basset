{
	"theme_support" : {
		"title-tag" : true,
		"html5" : [ 
			"search-form", 
			"comment-form", 
			"comment-list", 
			"gallery", 
			"caption", 
			"widgets" 
		],
		"post_formats" : [
			"status", 
			"quote", 
			"gallery", 
			"image", 
			"video", 
			"audio",
			"link", 
			"aside", 
			"chat"
		],
		"post-thumbnails" : [
			"post",
			"page",
			"movie"
		],
		"automatic-feed-links" : true
	},
	"scripts" : {
		"jquery" : false,
		"theme" : {
			"path" : "libraries/theme.js",
			"dependancies" : ["jquery"],
			"version" : false,
			"in_footer" : false
		}
	},
	"styles" : {
		"main" : {
			"path" : "style.less"
		}
	},
	"nav_menus" : {
		"header" : "Header Menu",
		"footer" : "Footer Menu"
	},
	"sidebars" : {
		"default" : {
			"name" : "Default Sidebar",
			"description" : "A sidebar for all pages",
			"class" : "default-sidebar",
			"before_title" : "",
			"after_title" : "",
			"before_widget" : "",
			"after_widget" : ""
		}
	},
	"image_sizes" : {
		"blog-thumb" : {
			"width" : 670,
			"height" : 230,
			"crop" : true
		},
		"home-boxes" : {
			"width" : 320,
			"height" : 120,
			"crop" : ["left", "top"]
		}
	},
	"customizer" : {},
	"meta_tags" : {
		"chars" : {
			"charset" : "UTF-8"
		},
		"ie-compat" : {
			"content" : "IE=edge",
			"http-equiv" : "X-UA-Compatible"
		},
		"viewport" : "width=device-width, initial-scale=1.0",
		"referrer" : "always"
	}
